# Patterns to ignore when building packages.
# This supports shell glob matching, relative path matching, and
# negation (prefixed with !). Only one pattern per line.
.DS_Store
# Common VCS dirs
.git/
.gitignore
.bzr/
.bzrignore
.hg/
.hgignore
.svn/
# Common backup files
*.swp
*.bak
*.tmp
*.orig
*~
# Various IDEs
.project
.idea/
*.tmproj
.vscode/
# OS generated files
Thumbs.db
# Helm
*.tgz
.helmignore
# CI/CD
.github/
.gitlab-ci.yml
.travis.yml
.circleci/
# Documentation
README.md.gotmpl
# Tests
tests/
ci/
# Example files
examples/
# Chart dependencies
charts/
requirements.lock
# Test results
tests/results/
*.log
# Documentation source
*.md.gotmpl 